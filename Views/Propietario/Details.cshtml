@model Inmobiliaria10.Models.Propietario

@{
    ViewData["Title"] = "Detalles Propietario";
}

<h2>Detalles Propietario</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Documento</dt>
        <dd class="col-sm-10">@Model.Documento</dd>

        <dt class="col-sm-2">Apellido y Nombres</dt>
        <dd class="col-sm-10">@Model.ApellidoNombres</dd>

        <dt class="col-sm-2">Domicilio</dt>
        <dd class="col-sm-10">@Model.Domicilio</dd>

        <dt class="col-sm-2">Tel√©fono</dt>
        <dd class="col-sm-10">@Model.Telefono</dd>

        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Email</dd>
    </dl>
</div>
<a asp-action="Edit" asp-route-id="@Model.IdPropietario" class="btn btn-primary">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Volver</a>
