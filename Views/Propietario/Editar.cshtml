@model Inmobiliaria10.Models.Propietario

@{
    ViewData["Title"] = "Editar Propietario";
}

<h2>Editar Propietario</h2>

<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="IdPropietario" />
    <div class="form-group">
        <label asp-for="Documento"></label>
        <input asp-for="Documento" class="form-control" />
        <span asp-validation-for="Documento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ApellidoNombres"></label>
        <input asp-for="ApellidoNombres" class="form-control" />
        <span asp-validation-for="ApellidoNombres" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Domicilio"></label>
        <input asp-for="Domicilio" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Telefono"></label>
        <input asp-for="Telefono" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Guardar</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}